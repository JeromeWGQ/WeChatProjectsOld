/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

button {
  background: initial;
  color: #412f1f;
}

button:focus {
  outline: 0;
}

button::after {
  border: none;
}


page {
  background: #a39e93;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.m-area {
  width: 100%;
}

.m-view {
  width: fit-content;
  height: fit-content;
}